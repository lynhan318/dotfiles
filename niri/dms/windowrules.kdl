window-rule {
    match app-id="^org.telegram.desktop$"
    open-floating true
    open-on-output "HDMI-A-1"
    open-on-workspace "1"
    block-out-from "screencast"
    min-width 600
    max-width 1000
    min-height 1000
    max-height 1000
}
